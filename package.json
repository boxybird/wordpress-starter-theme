    "resolve-url-loader": "^2.3.2",
    "sass": "^1.27.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.9.5",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1"
  }
